                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "fullname",
                        "in": "formData",
                        "description": "Name of register user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "username",
                        "in": "formData",
                        "description": "Username of register user",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "email",
                        "in": "formData",
                        "description": "Email of register user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "phone",
                        "in": "formData",
                        "description": "Phone of register user",
                        "required": true,
                        "type": "number"
                    },
                    {
                        "name": "password",
                        "in": "formData",
                        "description": "Password of register user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "country",
                        "in": "formData",
                        "description": "Country of register user",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "city",
                        "in": "formData",
                        "description": "City of register user",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "address",
                        "in": "formData",
                        "description": "Address of register user",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "camera",
                        "in": "formData",
                        "description": "Camera if yes 1 else 0",
                        "required": false,
                        "type": "number"
                    },
                    {
                        "name": "image",
                        "in": "formData",
                        "description": "Image of register user",
                        "required": true,
                        "type": "file"
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Request created successfully"
                    },
                    "400": {
                        "description": "Missing parameter"
                    },
                    "401": {
                        "description": "NOt authorize to complete this request"
                    }
                }
            }
        },
